{
  "expo": {
    "name": "S.A.F.E. Meds",
    "slug": "safe-meds-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "safemeds",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-router",
        {
          "origin": "https://safemeds.net"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}